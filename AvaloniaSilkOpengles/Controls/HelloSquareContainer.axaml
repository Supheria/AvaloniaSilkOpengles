<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="AvaloniaSilkOpengles.Controls.HelloSquareContainer"
    xmlns="https://github.com/avaloniaui"
    xmlns:controls="clr-namespace:AvaloniaSilkOpengles.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid RowDefinitions="Auto Auto *">
        <TextBlock x:Name="FpsCount" />
        <Button Content="De" Grid.Row="1" />
        <controls:HelloSquare Grid.Row="2" x:Name="HelloSquare" />
    </Grid>
</UserControl>
