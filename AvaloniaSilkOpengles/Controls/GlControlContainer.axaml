<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="AvaloniaSilkOpengles.Controls.GlControlContainer"
    xmlns="https://github.com/avaloniaui"
    xmlns:controls="clr-namespace:AvaloniaSilkOpengles.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid RowDefinitions="Auto Auto *">
        <TextBlock x:Name="Fps" />
        <TextBlock Grid.Row="1" x:Name="Mspf" />
        <ContentControl Grid.Row="2" x:Name="GlControl" />
    </Grid>
</UserControl>
